from typing import Any
from idlelib.editor import EditorWindow as EditorWindow
from tkinter import Tk, Variable

class FileList:
    root: Tk
    inversedict: dict[EditorWindow, str]
    vars: dict[str, Variable]
    dict: dict[str, EditorWindow]
    def __init__(self, root: Tk) -> None: ...
    def open(self, filename: str, action: bool | None = ...) -> EditorWindow | None: ...
    def gotofileline(self, filename: str, lineno: int | None = ...) -> None: ...
    def new(self, filename: str | None = ...) -> EditorWindow: ...
    def close_all_callback(self, *args: Any, **kwds: Any) -> str: ...
    def unregister_maybe_terminate(self, edit: EditorWindow) -> None: ...
    def filename_changed_edit(self, edit: EditorWindow) -> None: ...
    def canonize(self, filename: str) -> str: ...
