from re import Match, Pattern
from tkinter import BooleanVar, Misc, StringVar, Text

def get(root: Misc) -> SearchEngine: ...

class SearchEngine:
    root: Misc
    patvar: StringVar
    revar: BooleanVar
    casevar: BooleanVar
    wordvar: BooleanVar
    wrapvar: BooleanVar
    backvar: BooleanVar
    def __init__(self, root: Misc) -> None: ...
    def getpat(self) -> str: ...
    def setpat(self, pat: str) -> None: ...
    def isre(self) -> bool: ...
    def iscase(self) -> bool: ...
    def isword(self) -> bool: ...
    def iswrap(self) -> bool: ...
    def isback(self) -> bool: ...
    def setcookedpat(self, pat: str) -> None: ...
    def getcookedpat(self) -> str: ...
    def getprog(self) -> Pattern[str]: ...
    def report_error(self, pat: Pattern[str], msg: str, col: int | None = ...) -> None: ...
    def search_text(self, text: Text, prog: Pattern[str] | None = ..., ok: int = ...) -> tuple[int, Match[str]]: ...
    def search_forward(
        self, text: Text, prog: Pattern[str], line: int, col: int, wrap: int, ok: int = ...
    ) -> tuple[int, Match[str]] | None: ...
    def search_backward(
        self, text: Text, prog: Pattern[str], line: int, col: int, wrap: int, ok: int = ...
    ) -> tuple[int, Match[str]] | None: ...

def search_reverse(prog: Pattern[str], chars: str, col: int) -> Match[str] | None: ...
def get_selection(text: Text) -> tuple[str, str]: ...
def get_line_col(index: str) -> tuple[int, int]: ...
