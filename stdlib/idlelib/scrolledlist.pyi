from idlelib import macosx as macosx
from tkinter import BaseWidget, Event, Listbox, Menu
from tkinter.ttk import Frame, Scrollbar
from typing import Any, Union

class ScrolledList:
    default: str
    master: BaseWidget
    frame: Frame
    vbar: Scrollbar
    listbox: Listbox
    def __init__(self, master: BaseWidget, **options: Any) -> None: ...
    def close(self) -> None: ...
    empty: int
    def clear(self) -> None: ...
    def append(self, item: str) -> None: ...
    def get(self, index: int) -> str: ...
    def click_event(self, event: "Event[Any]") -> str: ...
    def double_click_event(self, event: Union["Event[Any]", None]) -> str: ...
    menu: Menu | None
    def popup_event(self, event: Union["Event[Any]", None]) -> str | None: ...
    def make_menu(self) -> None: ...
    def up_event(self, event: Union["Event[Any]", None]) -> str: ...
    def down_event(self, event: Union["Event[Any]", None]) -> str: ...
    def select(self, index: int) -> None: ...
    def fill_menu(self) -> None: ...
    def on_select(self, index: int) -> None: ...
    def on_double(self, index: int) -> None: ...
