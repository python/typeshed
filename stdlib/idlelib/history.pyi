from idlelib.config import idleConf as idleConf
from tkinter import Event, Text
from typing import Any, Union

class History:
    text: Text  # Idle Multicall wrapper of tk Text widget, with .bell().
    history: list[str]
    prefix: str | None
    pointer: int | None
    cyclic: bool
    def __init__(self, text: Text) -> None: ...
    def history_next(self, event: Union["Event[Any]", None]) -> str: ...
    def history_prev(self, event: Union["Event[Any]", None]) -> str: ...
    def fetch(self, reverse: bool) -> None: ...
    def store(self, source: str) -> None: ...
