from _typeshed import ReadableBuffer, SupportsNoArgReadline
from typing import Protocol
from typing_extensions import Reader, Writer

__all__ = ["encode", "decode", "encodestring", "decodestring"]

class _Input(Reader[bytes], SupportsNoArgReadline[bytes], Protocol): ...

def encode(input: _Input, output: Writer[bytes], quotetabs: int, header: bool = False) -> None: ...
def encodestring(s: ReadableBuffer, quotetabs: bool = False, header: bool = False) -> bytes: ...
def decode(input: _Input, output: Writer[bytes], header: bool = False) -> None: ...
def decodestring(s: str | ReadableBuffer, header: bool = False) -> bytes: ...
