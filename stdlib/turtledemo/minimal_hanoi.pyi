from turtle import Turtle

class Disc(Turtle):
    def __init__(self, n: int) -> None: ...

class Tower(list[Disc]):
    x: int
    def __init__(self, x: int) -> None: ...
    def push(self, d: Disc) -> None: ...
    def pop(self) -> Disc: ...  # type: ignore[override]

def hanoi(n: int, from_: Tower, with_: Tower, to_: Tower) -> None: ...
def play() -> None: ...
def main() -> str: ...
