    def wheel(self, initpos: Vec2D, scale: float) -> None: ...
    def tripiece(self, initpos: Vec2D, scale: float) -> None: ...
    def pentpiece(self, initpos: Vec2D, scale: float) -> None: ...
    def pentl(self, side: float, ang: float, scale: float) -> None: ...
    def pentr(self, side: float, ang: float, scale: float) -> None: ...
    def tripolyr(self, side: float, scale: float) -> None: ...
    def tripolyl(self, side: float, scale: float) -> None: ...
    def centerpiece(self, s: float, a: float, scale: float) -> None: ...

def main() -> str: ...
