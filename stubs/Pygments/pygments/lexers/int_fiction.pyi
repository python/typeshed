from pygments.lexer import RegexLexer
from typing import Any

class Inform6Lexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    flags: Any = ...
    tokens: Any = ...
    def get_tokens_unprocessed(self, text: Any) -> None: ...
    def analyse_text(text: Any): ...

class Inform7Lexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    flags: Any = ...
    tokens: Any = ...
    token_variants: Any = ...
    def __init__(self, **options: Any) -> None: ...

class Inform6TemplateLexer(Inform7Lexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    def get_tokens_unprocessed(self, text: Any, stack: Any = ...): ...

class Tads3Lexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    flags: Any = ...
    tokens: Any = ...
    def get_tokens_unprocessed(self, text: Any, **kwargs: Any) -> None: ...
    def analyse_text(text: Any): ...
