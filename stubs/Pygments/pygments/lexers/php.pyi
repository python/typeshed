from pygments.lexer import Lexer, RegexLexer
from typing import Any

class ZephirLexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    zephir_keywords: Any = ...
    zephir_type: Any = ...
    flags: Any = ...
    tokens: Any = ...

class PsyshConsoleLexer(Lexer):
    name: str = ...
    aliases: Any = ...
    def __init__(self, **options: Any) -> None: ...
    def get_tokens_unprocessed(self, text: Any) -> None: ...

class PhpLexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    mimetypes: Any = ...
    flags: Any = ...
    tokens: Any = ...
    funcnamehighlighting: Any = ...
    disabledmodules: Any = ...
    startinline: Any = ...
    def __init__(self, **options: Any) -> None: ...
    def get_tokens_unprocessed(self, text: Any) -> None: ...
    def analyse_text(text: Any): ...
