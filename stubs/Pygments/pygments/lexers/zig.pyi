from pygments.lexer import RegexLexer
from typing import Any

class ZigLexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    mimetypes: Any = ...
    type_keywords: Any = ...
    storage_keywords: Any = ...
    structure_keywords: Any = ...
    statement_keywords: Any = ...
    conditional_keywords: Any = ...
    repeat_keywords: Any = ...
    other_keywords: Any = ...
    constant_keywords: Any = ...
    tokens: Any = ...
