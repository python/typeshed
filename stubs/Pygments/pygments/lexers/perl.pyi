from pygments.lexer import ExtendedRegexLexer, RegexLexer
from typing import Any

class PerlLexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    mimetypes: Any = ...
    flags: Any = ...
    tokens: Any = ...
    def analyse_text(text: Any): ...

class Perl6Lexer(ExtendedRegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    mimetypes: Any = ...
    flags: Any = ...
    PERL6_IDENTIFIER_RANGE: str = ...
    PERL6_KEYWORDS: Any = ...
    PERL6_BUILTINS: Any = ...
    PERL6_BUILTIN_CLASSES: Any = ...
    PERL6_OPERATORS: Any = ...
    PERL6_BRACKETS: Any = ...
    def brackets_callback(token_class: Any): ...
    def opening_brace_callback(lexer: Any, match: Any, context: Any) -> None: ...
    def closing_brace_callback(lexer: Any, match: Any, context: Any) -> None: ...
    def embedded_perl6_callback(lexer: Any, match: Any, context: Any) -> None: ...
    tokens: Any = ...
    def analyse_text(text: Any): ...
    encoding: Any = ...
    def __init__(self, **options: Any) -> None: ...
