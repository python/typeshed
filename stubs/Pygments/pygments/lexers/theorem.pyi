from pygments.lexer import RegexLexer
from typing import Any

class CoqLexer(RegexLexer):
    name: str = ...
    aliases: Any = ...
    filenames: Any = ...
    mimetypes: Any = ...
    flags: Any = ...
    keywords1: Any = ...
    keywords2: Any = ...
    keywords3: Any = ...
    keywords4: Any = ...
    keywords5: Any = ...
    keywords6: Any = ...
    keyopts: Any = ...
    operators: str = ...
    prefix_syms: str = ...
    infix_syms: str = ...
    tokens: Any = ...
    def analyse_text(text: Any): ...

