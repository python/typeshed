from _typeshed import Incomplete

from docker.models.containers import Container

from .resource import Collection, Model

class Network(Model):
    @property
    def name(self): ...
    @property
    def containers(self): ...
    def connect(self, container: str | Container, *args, **kwargs): ...
    def disconnect(self, container: str | Container, *args, **kwargs): ...
    def remove(self): ...

class NetworkCollection(Collection):
    model: type[Network]
    def create(self, name: str, *args, **kwargs): ...
    def get(self, network_id: str, *args, **kwargs): ...
    def list(self, *args, **kwargs): ...
    def prune(self, filters: Incomplete | None = None): ...
