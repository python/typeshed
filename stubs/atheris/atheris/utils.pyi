from typing import IO

def path() -> str: ...

class ProgressRenderer:
    def __init__(self, stream: IO[str], total_count: int) -> None: ...
    def render(self) -> None: ...
    def erase(self) -> None: ...
    def drop(self) -> None: ...
    @property
    def count(self) -> int: ...
    @count.setter
    def count(self, new_count: int) -> None: ...
