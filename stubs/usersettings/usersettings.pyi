from typing import Any, TypeVar

_S = TypeVar("_S")

class Settings(dict[str, object]):
    app_id: str | None
    settings_directory: str | None
    settings_file: str | None
    def __init__(self, app_id: str) -> None: ...
    def add_setting(self, setting_name: str, setting_type: type[_S] = ..., default: _S | None = ...) -> None: ...
    def load_settings(self) -> None: ...
    def save_settings(self) -> None: ...
    def __getattr__(self, setting_name: str) -> Any: ...
    def __setattr__(self, setting_name: str, setting_value: object) -> None: ...
