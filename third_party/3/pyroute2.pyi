from typing import Dict, List, Any, Optional

class IPRoute(object):
    def __init__(self) -> None: ...
    def __enter__(self) -> IPRoute: ...
    def __exit__(self, exc_type, exc_value, traceback) -> None: ...
    def get_addr(self, address: Optional[str]=None, scope: Optional[int]=None,
                 index: Optional[int]=None, family: Optional[int]=None) -> list: ...
    def link_lookup(self, ifname: Optional[str]=None) -> list[int]: ...
    def close(self) -> None: ...
