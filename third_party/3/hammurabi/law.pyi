from hammurabi.config import config as config
from hammurabi.exceptions import AbortLawError as AbortLawError
from hammurabi.helpers import full_strip as full_strip
from hammurabi.mixins import GitMixin as GitMixin
from hammurabi.rules.base import Rule as Rule
from typing import Any, Iterable, List

class Law(GitMixin):
    name: Any = ...
    description: Any = ...
    rules: Any = ...
    def __init__(self, name: str, description: str, rules: Iterable[Rule]) -> Any: ...
    @property
    def documentation(self) -> str: ...
    def get_execution_order(self) -> List[Rule]: ...
    def commit(self) -> None: ...
    @staticmethod
    def __execute_rule(rule: Rule) -> Any: ...
    def enforce(self) -> None: ...
